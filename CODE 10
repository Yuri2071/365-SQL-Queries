--LAST LOG IN DE USUARIOS ENABLED

SELECT DISTINCT		dbo.SYSUSERLOG.USERID, dbo.USERINFO.ID, dbo.USERINFO.NAME, dbo.USERINFO.NETWORKALIAS,
					dbo.SYSUSERLOG.LOGOUTDATETIME
FROM				dbo.USERINFO
					LEFT JOIN dbo.SYSUSERLOG ON dbo.SYSUSERLOG.USERID=dbo.USERINFO.ID

WHERE				(dbo.SYSUSERLOG.LOGOUTDATETIME = (SELECT MAX(dbo.SYSUSERLOG.LOGOUTDATETIME) 
					FROM dbo.SYSUSERLOG WHERE dbo.SYSUSERLOG.USERID=dbo.USERINFO.ID))

					AND

					dbo.USERINFO.ENABLE = 1

					ORDER BY  dbo.SYSUSERLOG.LOGOUTDATETIME ASC
