--ENABLED USUARIOS QUE NUNCA SE CONECTARON

SELECT		dbo.SYSUSERLOG.USERID, dbo.USERINFO.ID, dbo.USERINFO.NAME, dbo.USERINFO.NETWORKALIAS,
			dbo.SYSUSERLOG.LOGOUTDATETIME,[enable]
FROM		dbo.USERINFO
			LEFT JOIN dbo.SYSUSERLOG ON dbo.SYSUSERLOG.USERID=dbo.USERINFO.ID

WHERE		dbo.SYSUSERLOG.LOGOUTDATETIME IS NULL

AND dbo.USERINFO.ENABLE = 1
